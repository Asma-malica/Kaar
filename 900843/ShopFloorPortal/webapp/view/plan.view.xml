<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="NITHYASHOPFLOOR1NITHYASHOPFLOOR1.controller.plan" xmlns:html="http://www.w3.org/1999/xhtml">
	<App id="app">
		<pages>
			<Page title="PLANNED ORDER">
				<Button width="39.375rem" text="PRODUCTION ORDER" type="Emphasized" press="onLogins"></Button>
				<Button width="39.375rem" text="PLANNED ORDER" type="Emphasized" press="onLogin"></Button>

				<content>
					<Table id="producttable" items="{/results}">
						<columns>
							<Column width="4em">
								<Text text="OrdNo"/>
							</Column>
							<Column width="4em">
								<Text text="Material"/>
							</Column>
							 <Column width="4em">
								<Text text="PlanPlant"/>
							</Column>
							<Column width="4em">
								<Text text="ProdPlant"/>
							</Column>
							<Column width="4em">
								<Text text="OrderStartDate"/>
							</Column>
							
							<Column width="4em">
								<Text text="OrderFinishDate"/>
							</Column>
								<Column width="4em">
								<Text text="GoodsRecptProcTime"/>
							</Column>
								<Column width="4em">
								<Text text="MrpController"/>
							</Column>
							<Column width="4em">
							    <Text text="ProdStartDate"/>
					    	</Column>
							<Column width="4em">
								<Text text="ProdFinishDate"/>
							</Column>
							
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{PlannedorderNum}"/>
								    <Text text="{Material}"/>
								    <Text text="{PlanPlant}"/>
								    <Text text="{ProdPlant}"/>
	                                <Text text="{OrderStartDate}"/>
								    <Text text="{OrderFinDate}"/>
							        <Text text="{GrProcTime}"/>
									<Text text="{MrpController}"/>
								    <Text text="{ProdStartDate}"/>
								    <Text text="{ProdFinishDate}"/>
								    
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>