<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="NITHYASHOPFLOOR1NITHYASHOPFLOOR1.controller.pur" xmlns:html="http://www.w3.org/1999/xhtml">
	<App id="app">
		<pages>
			<Page title="PRODUCTION ORDER">
				<Button width="39.375rem" text="PRODUCTION ORDER" type="Emphasized" press="onLogins"></Button>
				<Button width="39.375rem" text="PLANNED ORDER" type="Emphasized" press="onLogin"></Button>

				<content>
					<Table id="producttable" items="{/results}">
						<columns>
							<Column width="4em">
								<Text text="OrdNo"/>
							</Column>
							<Column width="4em">
								<Text text="ProdPlant"/>
							</Column>
							 <Column width="4em">
								<Text text="Material"/>
							</Column>
							<Column width="4em">
								<Text text="BOMExplDate"/>
							</Column>
							<Column width="4em">
								<Text text="RoutingNo"/>
							</Column>
							
							<Column width="4em">
								<Text text="SchdReleaseDate"/>
							</Column>
								<Column width="4em">
								<Text text="Finished Date"/>
							</Column>
								<Column width="4em">
								<Text text="StartDate"/>
							</Column>
							<Column width="4em">
							    <Text text="Prod Finish Date"/>
					    	</Column>
						
							<Column width="4em">
								<Text text="Material Text"/>
							</Column>
							
							
						</columns>
						<items>
							<ColumnListItem>
								<cells>
								
								    
								    
								    	<Text text="{OrderNumber}"/>
								    <Text text="{ProductionPlant}"/>
								    <Text text="{Material}"/>
								    <Text text="{ExplDate}"/>
	                                <Text text="{RoutingNo}"/>
								    <Text text="{SchedReleaseDate}"/>
							        <Text text="{FinishDate}"/>
									<Text text="{StartDate}"/>
								    <Text text="{ProductionFinishDate}"/>
								   
								    <Text text="{MaterialText}"/>
								   
								    
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>